{
  "name": "quick-sip",
  "description": "Gulp build process tasks that provide watchify and sass compilation.",
  "repository": {
    "type": "git",
    "url": "https://github.com/vecnatechnologies/quick-sip"
  },
  "bugs": {
    "url": "https://github.com/vecnatechnologies/quick-sip/issues"
  },
  "license": "MIT",
  "version": "0.1.19",
  "dependencies": {
    "browserify": "13.1.0",
    "color-log": "0.0.2",
    "del": "2.2.2",
    "gulp-autoprefixer": "3.1.1",
    "gulp-concat": "2.6.0",
    "gulp-jshint": "^2.1.0",
    "gulp-load-plugins": "^1.6.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "1.7.1",
    "gulp-tap": "0.1.3",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "3.0.6",
    "jasmine": "3.1.0",
    "jshint": "^2.10.2",
    "jshint-stylish": "2.2.1",
    "lodash": "^4.17.12",
    "run-sequence": "1.1.1",
    "snyk": "^1.217.3",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "^3.11.1"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "scripts": {
    "test": "jasmine",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "release": "release-it"
  },
  "snyk": true,
  "devDependencies": {
    "release-it": "^12.3.5"
  },
  "release-it": {
    "github": {
      "release": true,
      "releaseName": "Release v${version}"
    },
    "git": {
      "tagName": "${version}",
      "commitMessage": "Release v${version}",
      "tagAnnotation": "Release v${version}"
    }
  }
}
